<?xml version="1.0"?>
<configuration>

  

  
  <!-- When deploying the service library project, the content of the config file must be added to the host's 
  app.config file. System.Configuration does not support config files for libraries. -->
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="netTcpBinding" sendTimeout="00:00:02"/>
      </netTcpBinding>
      <wsHttpBinding>
        <binding name="NoSecurityPlusRM">
          <reliableSession enabled="true"/>
          <security mode="None"/>
        </binding>
      </wsHttpBinding>
    </bindings>
    <client/>
    <services>
      <service behaviorConfiguration="Metadata" name="newWCFService.HelloService">
        
        <endpoint address="ws" binding="wsHttpBinding" bindingConfiguration="NoSecurityPlusRM"
          name="Helloservice_wshttp" contract="newWCFService.IHello" />
        <endpoint address="basic" binding="basicHttpBinding" name="Helloservice_basichttp"
          contract="newWCFService.IHello" />
        <endpoint address="net.tcp://localhost:8081/tcp" binding="netTcpBinding"
          bindingConfiguration="netTcpBinding" name="Helloservice_tcphttp"
          contract="newWCFService.IHello" />
        
        <endpoint address="http" behaviorConfiguration="Web" binding="webHttpBinding"
          name="Helloservice_webhttp" contract="newWCFService.IHello" />
        
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:5000/helloservice/" />
          </baseAddresses>
        </host>
      </service>
    </services>

    <behaviors>
      
      <endpointBehaviors>
        <behavior name="Web">
          <webHttp />
        </behavior>
      </endpointBehaviors>
      
      <serviceBehaviors>
        <behavior name="Metadata">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="http://localhost:8080/helloservice/meta" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
