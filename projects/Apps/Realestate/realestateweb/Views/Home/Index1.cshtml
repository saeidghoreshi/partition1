@{
    ViewBag.Title = "Index1";
    Layout = "~/Views/home/Master.cshtml";
}
                
<!--Layout-->
<div id="mainLayout" class="easyui-layout" style="width:100%;height:820px">  
    
    <!--Region West-->
    <div data-options="region:'west',title:'',split:true" style="width:250px;background:#f5f5f5">
         @{Html.RenderAction("getPanel", "home", new { });}

         
    </div>  

    <!--Region East-->
    <div data-options="region:'east',title:'Selected Area',split:true" style="width:250px;background:#f5f5f5">
         @{Html.RenderAction("getListings", "home", new { });}
    </div>  

    <!--Region Center-->
    <div id='regionCenter' data-options="region:'center',title:'Result'" style="padding:2px;">
        @RenderPage("index1/menu.cshtml")
        <!--Inner Layout-->
        <div id="mainLayout2" class="easyui-layout" style="width:100%;height:745px;margin-top:2px;"> 

            <!--Region South-->
            <div data-options="region:'south',title:' ',split:true" style="height:100px;background:#f5f5f5">
                 @{Html.RenderAction("getSouth", "home", new { });}
            </div>  
            <div id="layout2-center" data-options="region:'center',title:''" style="padding:0px;width:100%;height:100%">
    
                <div id="center-area" style="height:100%;width:100%;"></div>
               
            </div>

        </div>
        <!--Inner Layout-->
    </div> 
    
</div>  


<script src="../../Components/index1/mapHandler.js" type="text/javascript"></script>

<!--start Point-->
<link href="../../Components/index1/page.css" rel="stylesheet" type="text/css" />
<script src="../../Components/index1/page.js" type="text/javascript"></script>

