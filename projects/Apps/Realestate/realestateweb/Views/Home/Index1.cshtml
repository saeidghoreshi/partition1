@{
    ViewBag.Title = "Index1";
    Layout = "~/Views/home/Master.cshtml";
}



  <script type="text/javascript">
      (function ($) {
          $(document).ready(function () {
              var theme = getDemoTheme();
              $('#splitContainer').jqxSplitter({ height: "100%", width: "100%", orientation: 'horizontal', theme: theme, panels: [{ size: "5%" }, { size: '90%'}] });
              $('#splitter').jqxSplitter({ width: '100%', theme: theme, panels: [{ size: 200 }, { size: '70%' }, { size: 200}] });

              $("#tabs1").jqxTabs({ theme: theme, height: '100%', width: '100%' });

          });
      } (jQuery));
        
    </script>

<div class='default'>
    <div id="splitContainer">
        <div>
            North
        </div>
        <div id="splitter">
            <!--Left-->
            <div class="splitter-panel">
            
                @{Html.RenderAction("getPanel", "home", new { });}

            </div>

            <!--Center-->
            <div class="splitter-panel">
            <div id="tabs1">
                    <ul>
                        <li style="margin-left: 10px;">Tab1</li>
                        <li>Tab 2</li>
                    </ul>
                    <!--Tab1 Content-->
                    <div>
                    <div id="center-area" ></div>        
                    </div>
                    
                    <!--Tab2 Content-->
                    <div>
                        
                    </div>

                </div>
                
            </div>

            <!--Right-->
            <div class="splitter-panel">
                @{Html.RenderAction("getListings", "home", new { });}
            </div>
            
        </div>
        
        
    </div>
</div>



<script src="../../Components/index1/mapHandler.js" type="text/javascript"></script>

