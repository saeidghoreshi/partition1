@{
    ViewBag.Title = "Index1";
    Layout = "~/Views/home/Master.cshtml";
}



  <script type="text/javascript">
      (function ($) {
          $(document).ready(function () {
              var theme = getDemoTheme();

              $('#splitter').jqxSplitter({ width: '100%', orientation: 'vertical', height: "800", theme: theme, panels: [{ size: "15%" }, { size: '68%' }, { size: "20%"}] });

              $("#tabs1").jqxTabs({ theme: theme, height: '100%', width: '100%' });

          });
      } (jQuery));
        
    </script>

<div class='default'>

        <div id="splitter">
            <!--Left-->
            <div class="splitter-panel">
            
                @{Html.RenderAction("getPanel", "home", new { });}

            </div>

            <!--Center-->
            <div class="splitter-panel">
            <div id="tabs1">
                    <ul>
                        <li style="margin-left: 20px;">Map</li>
                        <li>List</li>
                    </ul>
                    <!--Tab1 Content-->
                    <div style="padding:3px">
                        <div id="center-area" style="width:100%;height:100%"></div>        
                    </div>
                    
                    <!--Tab2 Content-->
                    <div>
                        Tab2    
                    </div>
                </div>
                
            </div>

            <!--Right-->
            <div class="splitter-panel">
                @{Html.RenderAction("getListings", "home", new { });}
            </div>
            
        </div>
    
</div>


