<?xml version="1.0"?>
<configuration>

  <connectionStrings>
    <add name="AccContexts" connectionString="$(ReplacableToken_AccContexts-Web.config Connection String_0)" providerName="System.Data.EntityClient"/>
  </connectionStrings>
  
  <system.serviceModel>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true"/>
    <services>
      <service name="RyanGoreshi.AccountingV1" behaviorConfiguration="serviceBehavior">
        <endpoint address="I1" binding="basicHttpBinding" contract="RyanGoreshi.IAccountingV1"/>
        <!--RESTFUL-->
        <endpoint address="REST" binding="webHttpBinding" contract="RyanGoreshi.IAccountingV1" behaviorConfiguration="WEB"/>
        <endpoint contract="IMetadataExchange" binding="mexHttpBinding" address=""/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="serviceBehavior">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="/Accounting/meta"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <behavior name="WEB">
          <webHttp/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <add name="Access-Control-Allow-Origin" value="*"/>
      </customHeaders>
    </httpProtocol>
  </system.webServer>
  <system.web>
    <compilation/>
  </system.web>
</configuration>