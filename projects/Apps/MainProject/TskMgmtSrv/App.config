<?xml version="1.0"?>
<configuration>

  <system.web>
    <compilation debug="true"/>
  </system.web>
  
  
  <system.serviceModel>

    <behaviors>
      <serviceBehaviors>
        <behavior name="serviceBehavior">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="mex"/>
          <serviceDiscovery/>
        </behavior>
      </serviceBehaviors>
    </behaviors>

    

    <services>
      
      <service name="TskMgmtNS.TskMgmtservice" behaviorConfiguration="serviceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:1010/"/>
          </baseAddresses>
        </host>
        <endpoint address ="basic" binding="basicHttpBinding" contract="TskMgmtNS.ITskMgmtservice"></endpoint>
        <endpoint address="" contract="IMetadataExchange" binding="mexHttpBinding" />
      </service>
    </services>

  </system.serviceModel>


  <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
