<form id="permission_form" class="permission_form" enctype="multipart/form-data">
<table>
<tr>
    <td>
    <img  id="permission_curUser" width="50px" height="50px" src="../../../resources/components/base/images/user.png" />
    </td>
</tr>
<tr>
    <td>
    
        <div id="permission_options_container">
		    <input type="radio" id="permission_options_1" name="radio" /><label for="permission_options_1">By Name</label>
		    <input type="radio" id="permission_options_2" name="radio" checked="checked" /><label for="permission_options_2">By Severity</label>
	    </div>
    
    </td>
</tr>

<tr>
    <td>
    
    <div class="permission_section">
        <div class="permission_panel_header" >Existing Roles</div>
        <div id="s1" class="permission_panel permission_G1" >

            @foreach (var item in ViewBag.existingRoles)
            {
                <div class="permission_panel_items" id="user_@item.role_id" roleId=@item.role_id>@item.role_name</div>
            }
    
        </div>
    </div>

    <div class="permission_section">
        <div class="permission_panel_header" >Applied Roles</div>
        <div id="s2" class="permission_panel permission_G2" >
    
            @foreach (var item in ViewBag.existingUserAssignedRoles)
            {
                <!--ui-state-disabled-->
                <div class="permission_panel_items" id="user_@item.role_id" roleId=@item.role_id>@item.role_name</div>
            }
    
        </div>
    </div>

    </td>
    
</tr>
<tr>
        <td><input id="permission_save_button" type="button" value='Save'/></td>
</tr>
    
</table>
</form>
